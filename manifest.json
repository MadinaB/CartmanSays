{
    "name" : "CartmanSays" ,
    "description" : "Cartman is here to cheer you up" ,
    "manifest_version": 2,
    "version" : "1.0", 
    "browser_action":{
        "default_popup" : "popup.html",
        "default_icon" : "icon.png"
    },
    "options_page": "options.html",
    "background": { "scripts": ["background.js"] },
    "permissions" : ["notifications", "alarms", "storage"]
}
